<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="750.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="application.LoginController">
   <children>
      <Label layoutX="302.0" layoutY="110.0" text="Sign In">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <TextField fx:id="usernameInput" alignment="CENTER" layoutX="225.0" layoutY="250.0" prefHeight="25.0" prefWidth="300.0" promptText="Username" />
      <PasswordField fx:id="passwordInput" alignment="CENTER" layoutX="225.0" layoutY="292.0" prefHeight="25.0" prefWidth="300.0" promptText="Password" />
      <Hyperlink fx:id="forgottenPasswordHyperlink" layoutX="311.0" layoutY="416.0" text="Forgot your password?" />
      <Button fx:id="loginButton" layoutX="300.0" layoutY="356.0" mnemonicParsing="false" prefWidth="150.0" text="Login" />
      <Hyperlink fx:id="signUpHyperlink" layoutX="351.0" layoutY="393.0" text="Sign up" />
   </children>
</AnchorPane>
